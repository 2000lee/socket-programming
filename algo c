using System.Collections.Generic;
public class Solution
{
    public int[] TwoSum(int[] nums, int target)
    {
        int[] output = new int[2];
        for (int i = 0; i < nums.Length; i++)
        {
            for (int n = 0; n < nums.Length; n++)
            {
                if ((nums[i] + nums[n]) == target)
                {
                    output[0] = n;
                    output[1] = i;
                }
            }
        }
        return output;
    }
}